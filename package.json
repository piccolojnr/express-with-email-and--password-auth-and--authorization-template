{
  "name": "express-with-email-and -password-auth-and -authorization-template",
  "version": "1.0.0",
  "description": "A Node.js/TypeScript backend template with authentication and optional authorization",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npx tsc",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "type-check": "tsc --noEmit",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "prisma",
    "authentication",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jet-logger": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
